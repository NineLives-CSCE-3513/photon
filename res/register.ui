<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="root" named="True">
    <property name="geometry">300x300</property>
    <property name="height">300</property>
    <property name="resizable">none</property>
    <property name="title" translatable="yes">Photon Registration</property>
    <child>
      <object class="ttk.Frame" id="main_frame" named="True">
        <property name="height">352</property>
        <property name="width">288</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="side">top</property>
        </layout>
        <containerlayout manager="grid">
          <property name="anchor">center</property>
        </containerlayout>
        <child>
          <object class="tk.Button" id="submit_button" named="True">
            <property name="text" translatable="yes">Submit</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">10 10</property>
              <property name="row">8</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="username_label" named="True">
            <property name="text" translatable="yes">Username:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">10 0</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="username_entry" named="True">
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">0 10</property>
              <property name="row">5</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="last_name_label" named="True">
            <property name="text" translatable="yes">Last name:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">10 0</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="last_name_entry" named="True">
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">3</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="first_name_prompt" named="True">
            <property name="text" translatable="yes">First name:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">10 0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="first_name_entry" named="True">
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="equipment_prompt" named="True">
            <property name="text" translatable="yes">Equipment ID:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">6</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="equipment_entry" named="True">
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">7</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
